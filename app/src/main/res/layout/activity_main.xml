<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="horizontal"
    android:background="@drawable/opera">

    <View
        android:layout_marginTop="60dp"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="@android:color/darker_gray"/>

    <!-- ADMSENSAL -->
    <ImageView
        android:id="@+id/admCensal"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:id="@+id/admCensaltxt"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/admCensal"
        android:layout_marginLeft="10dp"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="13dp" />
    <!-- BROWSER -->
    <ImageView
        android:id="@+id/chromeButton"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@id/admCensal"/>
    <TextView
        android:textSize="13dp"
        android:gravity="center"
        android:id="@+id/chromeName"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@id/admCensal"
        android:layout_below="@id/chromeButton"/>
    <!-- MCC -->
    <ImageView
        android:id="@+id/mcc"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@id/chromeButton"/>
    <TextView
        android:textSize="13dp"
        android:gravity="center"
        android:id="@+id/mccName"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:textColor="#000000"
        android:layout_toRightOf="@id/chromeButton"
        android:layout_below="@id/mcc" />
    <!-- CAPACITACIÃ“N -->
    <ImageView
        android:id="@+id/capaButton"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@id/mcc"/>
    <TextView
        android:textSize="13dp"
        android:gravity="center"
        android:id="@+id/capaName"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@id/mcc"
        android:layout_below="@id/capaButton"/>
    <!-- EXPLORER -->
    <ImageView
        android:id="@+id/explorer"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="80dp"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@id/capaButton"/>
    <TextView
        android:textSize="13dp"
        android:gravity="center"
        android:id="@+id/explorerName"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:textColor="#000000"
        android:layout_toRightOf="@id/capaButton"
        android:layout_below="@id/explorer" />

    <!-- CARTO -->
    <ImageView
        android:id="@+id/transfer"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/explorer"
        android:layout_marginLeft="20dp"
        />
    <TextView
        android:textSize="13dp"
        android:gravity="center"
        android:id="@+id/transferName"
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_marginLeft="20dp"
        android:layout_below="@id/transfer"/>


    <!--  Barra   -->

    <ImageView
        android:id="@+id/settingsWifi"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:id="@+id/settingsBluetooth"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/settingsWifi"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="15dp"/>
    <ImageView
        android:id="@+id/settings3G"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/settingsBluetooth"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="15dp"/>
    <ImageView
        android:id="@+id/settingsLocation"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/settings3G"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="15dp"/>
    <ImageView
        android:id="@+id/call"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/settingsLocation"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:id="@+id/sms"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/call"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:id="@+id/cameraSettings"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="22dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/sms"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:id="@+id/settingsAll"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="22dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/cameraSettings"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:id="@+id/lock"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="22dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/settingsAll"
        android:layout_marginTop="15dp"/>

    <ImageView
        android:id="@+id/unlock"
        android:layout_width="30dp"
        android:layout_height="30dp"
       android:layout_marginLeft="22dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/settingsAll"
        android:layout_marginTop="15dp"/>
</RelativeLayout>